{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "**/*.@(jpg|jpeg|gif|png|js)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=604800000"
      }]
    }],
    "rewrites": [{
      "source": "**",
      "function": "ssrapp"
    }]
  },
  "functions": {
    "source": "functions"
  }
}
